{
  "domain": "mylight_bt",
  "name": "MyLight bluetooth",
  "config_flow": true,
  "documentation": "https://github.com/oskarjoelsson/hass-bluetooth-lightbulb-speaker",
  "issue_tracker": "https://github.com/oskarjoelsson/hass-bluetooth-lightbulb-speaker/issues",
  "codeowners": [
    "@orrpan"
  ],
  "dependencies": [
    "bluetooth"
  ],
  "requirements": [
    "bleak>=0.18.0",
    "bleak-retry-connector>=2.1.3",
    "git+https://github.com/oskarjoelsson/mylight.git@reconnect#mylight==0.0.31"
  ],
  "version": "0.0.2",
  "iot_class": "local_polling",
  "loggers": [
    "bleak"
  ],
  "bluetooth": [
    {
      "connectable": true,
      "local_name": "Badrum app"
    },
    {
      "connectable": true,
      "local_name": "MyLight app"
    }
  ]
}